#%RAML 0.8 ResourceType

get?:
  responses: &standardResponses
    200:
      description: OK
    404:
      description: Entity not found
    500:
        description: Internal server error
put?:
  responses: *standardResponses
patch?:
  responses: *standardResponses
post?:
    responses:
      201:
        description: Created      
delete?:
  responses: *standardResponses